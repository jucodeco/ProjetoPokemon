<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background"
    tools:context=".Login">


<View
        android:id="@+id/containtercompo"
        style="@style/container_compo"


        android:layout_width="1040dp"
        android:layout_height="245dp"
        tools:layout_editor_absoluteX="-60dp"
        tools:layout_editor_absoluteY="315dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/edit_email"
        style="@style/edit_text"
        android:layout_marginTop="24dp"
        android:hint="@string/email"
        app:layout_constraintEnd_toEndOf="@id/containtercompo"
        app:layout_constraintHorizontal_bias="0.11"
        app:layout_constraintStart_toStartOf="@id/containtercompo"
        app:layout_constraintTop_toTopOf="@id/containtercompo"
        android:importantForAutofill="no" />

    <EditText
        android:id="@+id/edit_senha"
        style="@style/edit_text"
        android:layout_marginTop="48dp"
        android:hint="Senha"
        app:layout_constraintEnd_toEndOf="@id/containtercompo"
        app:layout_constraintHorizontal_bias="0.105"
        app:layout_constraintStart_toStartOf="@id/containtercompo"
        app:layout_constraintTop_toBottomOf="@id/edit_email"
        android:importantForAutofill="no"
        android:autofillHints="" />
    <androidx.appcompat.widget.AppCompatButton
        style="@style/button"
        android:text="Entrar"
        tools:layout_editor_absoluteX="130dp"
        tools:layout_editor_absoluteY="599dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>